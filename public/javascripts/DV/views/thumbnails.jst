<% _.each(thumbnails, function(thumbnail, page) { %>
  <div class="DV-thumbnail DV-zoom-<%= zoom %>" id="DV-thumbnail-<%= page %>">
    <div class="DV-thumbnail-image"><img src="<%= thumbnail %>" /></div>
    <div class="DV-thumbnail-page DV-pageNumber DV-pageMeta">p. <%= page %></div>
  </div>
<% }); %>
